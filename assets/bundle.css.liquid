/* ------------------------------------------------------------ *\
Accordions
\* ------------------------------------------------------------ */
.accordions {
  border-top: 1px solid #fff;
  border-bottom: 1px solid #fff; }
.accordions.accordions--product-plans {
  border-top: 0;
  border-bottom: 0; }
.accordions.accordions--product-plans .accordion {
  border: 1px solid rgba(255, 255, 255, 0.5); }
.accordions.accordions--product-plans .accordion + .accordion {
  margin-top: 15px; }
.accordions.accordions--product-plans .accordion.active .accordion__head {
  background: rgba(255, 255, 255, 0.3); }
.accordions.accordions--product-plans .accordion.active .accordion__head:after {
  right: 10px; }
.accordions.accordions--product-plans .accordion .accordion__head {
  padding: 11px 15px;
  -webkit-transition: background 0.3s;
  -o-transition: background 0.3s;
  transition: background 0.3s;
  position: relative; }
.accordions.accordions--product-plans .accordion .accordion__head:after {
  right: 20px; }
.accordions.accordions--product-plans .accordion .accordion__head:hover {
  background: rgba(255, 255, 255, 0.3); }
.accordions.accordions--product-plans .accordion .accordion__head a {
  text-decoration: none; }
.accordions.accordions--product-plans .accordion .accordion__head h3 {
  font-family: Oswald, Helvetica, sans-serif;
  font-style: normal;
  font-weight: 600;
  line-height: normal;
  font-size: 26px;
  text-transform: uppercase; }
.accordions.accordions--product-plans .accordion .accordion__head p {
  font-style: normal;
  font-weight: normal;
  font-size: 18px;
  letter-spacing: 1px; }
.accordions.accordions--product-plans .accordion .accordion__body {
  display: none; }
.accordions .accordion + .accordion {
  border-top: 1px solid rgba(255, 255, 255, 0.5); }
.product_accordion.accordions .accordion + .accordion {
    border-top: 1px solid #fff; }
.accordions .accordion.active .accordion__head:after {
  -webkit-transform: rotate(-135deg) translateY(-50%);
  -ms-transform: rotate(-135deg) translateY(-50%);
  transform: rotate(-135deg) translateY(-50%);
  right: 0; }
.accordions .accordion .accordion__head {
  padding: 13px 0;
  position: relative; }
.accordions .accordion .accordion__head:after {
  content: '';
  display: block;
  height: 15px;
  width: 15px;
  border: 2px solid #fff;
  -webkit-transform: rotate(45deg) translateY(-50%);
  -ms-transform: rotate(45deg) translateY(-50%);
  transform: rotate(45deg) translateY(-50%);
  border-left: 0;
  border-top: 0;
  pointer-events: none;
  position: absolute;
  top: calc(50% - 7px);
  right: 10px;
  -webkit-transition: -webkit-transform 0.3s;
  transition: -webkit-transform 0.3s;
  -o-transition: transform 0.3s;
  transition: transform 0.3s;
  transition: transform 0.3s, -webkit-transform 0.3s; }
.accordions .accordion .accordion__head a {
  text-decoration: none;
  display: block;
  width: 100%;
  height: 100%;
  z-index: 1; }
.accordions .accordion .accordion__head h3 {
  font-family: Oswald, Helvetica, sans-serif;
  font-style: normal;
  font-weight: 600;
  line-height: normal;
  font-size: 26px;
  margin-bottom: 0;
  text-transform: uppercase; }
.accordions .accordion .accordion__body {
  display: none;
  font-style: normal;
  font-weight: normal;
  line-height: 1;
  font-size: 18px;
  margin-bottom: 0; }
.accordions .accordion .accordion__body p {
  color: #fff; }
@media (max-width: 560px) {
